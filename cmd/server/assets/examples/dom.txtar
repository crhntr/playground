package main

import (
	_ "embed"

	"github.com/crhntr/dom/browser"
)

//go:embed body.html
var bodyHTML string

func main() {
	document := browser.OpenDocument()
	body := document.Body()
	body.SetInnerHTML(bodyHTML)
}
